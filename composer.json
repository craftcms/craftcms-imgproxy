{
  "name": "craftcms/plugin-skeleton",
  "description": "An example plugin for Craft CMS with tests",
  "authors": [
    {
      "name": "Jason McCallister",
      "email": "jason@craftcms.com"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "craftcms/cms": "^3.6.0-RC1",
    "vlucas/phpdotenv": "^2.4.0",
    "yiisoft/yii2": "^2.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.3",
    "codeception/module-rest": "^1.2",
    "codeception/module-phpbrowser": "^1.0",
    "codeception/module-yii2": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PluginName\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests"
    }
  }
}
